        <ORIGIN>United States</ORIGIN>
        <ALPHA>15.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>31.0</AMOUNT>
        <DISPLAY_AMOUNT>31.0 g</DISPLAY_AMOUNT>
