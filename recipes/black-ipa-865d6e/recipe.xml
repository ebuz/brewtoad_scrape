    <HOPS>
      <HOP>
        <NAME>Northern Brewer (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>9.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>30.0</AMOUNT>
