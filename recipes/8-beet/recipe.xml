    <HOPS>
      <HOP>
        <NAME>Cascade (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>7.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>12.0</AMOUNT>
