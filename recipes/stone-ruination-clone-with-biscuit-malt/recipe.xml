    <HOPS>
      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>15.4</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.04961167851661809</AMOUNT>
