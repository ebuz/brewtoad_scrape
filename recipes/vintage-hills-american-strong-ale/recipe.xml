    <HOPS>
      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>12.9</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.035436875</AMOUNT>
