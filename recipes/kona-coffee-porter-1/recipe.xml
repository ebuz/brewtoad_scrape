    <HOPS>
      <HOP>
        <NAME>Cluster (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>6.8</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.056699</AMOUNT>
