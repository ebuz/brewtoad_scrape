      <HOP>
        <NAME>Cascade (NZ)</NAME>
        <ORIGIN>New Zealand</ORIGIN>
        <ALPHA>9.2</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.025000006575</AMOUNT>
