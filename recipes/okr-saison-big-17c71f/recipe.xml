    <HOPS>
      <HOP>
        <NAME>Saaz (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>3.75</ALPHA>
        <BETA></BETA>
        <AMOUNT>130.0</AMOUNT>
        <DISPLAY_AMOUNT>130.0 g</DISPLAY_AMOUNT>
