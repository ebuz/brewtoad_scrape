    <HOPS>
      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>17.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>11.0</AMOUNT>
