    <HOPS>
      <HOP>
        <NAME>Warrior (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>15.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.17009718348554748</AMOUNT>
