    <HOPS>
      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>15.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.021262124999999996</AMOUNT>
