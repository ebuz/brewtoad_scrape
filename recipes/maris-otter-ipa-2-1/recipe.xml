    <HOPS>
      <HOP>
        <NAME>Galena (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>13.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.049611625</AMOUNT>
