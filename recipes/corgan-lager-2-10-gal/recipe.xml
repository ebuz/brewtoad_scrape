    <HOPS>
      <HOP>
        <NAME>Williamette (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>5.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.056699</AMOUNT>
