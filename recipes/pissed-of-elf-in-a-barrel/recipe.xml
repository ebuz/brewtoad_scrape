    <HOPS>
      <HOP>
        <NAME>Nugget (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>14.3</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.0283495</AMOUNT>
