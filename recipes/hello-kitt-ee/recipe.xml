    <HOPS>
      <HOP>
        <NAME>Apollo (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>19.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>15.0000161806687</AMOUNT>
