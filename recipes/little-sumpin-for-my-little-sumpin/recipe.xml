      <HOP>
        <NAME>Simcoe (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>13.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>28.0</AMOUNT>
        <DISPLAY_AMOUNT>28.0 g</DISPLAY_AMOUNT>
