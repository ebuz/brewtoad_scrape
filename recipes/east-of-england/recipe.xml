    <HOPS>
      <HOP>
        <NAME>Bramling Cross (UK)</NAME>
        <ORIGIN>United Kingdom</ORIGIN>
        <ALPHA>6.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.03543691322615565</AMOUNT>
