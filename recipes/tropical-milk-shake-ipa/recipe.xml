    <HOPS>
      <HOP>
        <NAME>Chinook (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>12.5</ALPHA>
        <BETA></BETA>
        <AMOUNT>50.0</AMOUNT>
