    <HOPS>
      <HOP>
        <NAME>Chinook (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>13.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.01417475</AMOUNT>
