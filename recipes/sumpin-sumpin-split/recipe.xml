      <HOP>
        <NAME>Williamette (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>6.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.0100640725</AMOUNT>
