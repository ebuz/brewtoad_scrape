    <HOPS>
      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>14.6</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.013000003419000001</AMOUNT>
