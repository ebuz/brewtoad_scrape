    <HOPS>
      <HOP>
        <NAME>Cascade (NZ)</NAME>
        <ORIGIN>New Zealand</ORIGIN>
        <ALPHA>5.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.024000006312</AMOUNT>
