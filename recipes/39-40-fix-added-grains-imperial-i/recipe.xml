      <HOP>
        <NAME>Amarillo (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>9.4</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.12757288761416047</AMOUNT>
