    <HOPS>
      <HOP>
        <NAME>Cascade (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>5.75</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.056699</AMOUNT>
