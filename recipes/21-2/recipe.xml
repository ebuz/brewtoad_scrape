    <HOPS>
      <HOP>
        <NAME>Centennial (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>10.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.0028349530580924582</AMOUNT>
