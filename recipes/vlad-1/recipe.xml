    <HOPS>
      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>15.6</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.1417475</AMOUNT>
