    <HOPS>
      <HOP>
        <NAME>Citra (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>13.65</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.056699</AMOUNT>
