    <HOPS>
      <HOP>
        <NAME>Chinook (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>12.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>80.0</AMOUNT>
