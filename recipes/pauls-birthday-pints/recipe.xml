    <HOPS>
      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>16.1</ALPHA>
        <BETA></BETA>
        <AMOUNT>50.0</AMOUNT>
