    <HOPS>
      <HOP>
        <NAME>Warrior (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>16.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.00283495</AMOUNT>
