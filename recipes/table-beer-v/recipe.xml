      <HOP>
        <NAME>Cascade (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>7.4</ALPHA>
        <BETA></BETA>
        <AMOUNT>15.02524725625</AMOUNT>
