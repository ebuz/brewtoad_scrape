    <HOPS>
      <HOP>
        <NAME>Cascade (NZ)</NAME>
        <ORIGIN>New Zealand</ORIGIN>
        <ALPHA>7.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>30.0</AMOUNT>
