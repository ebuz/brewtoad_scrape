    <HOPS>
      <HOP>
        <NAME>Summit (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>18.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.034869884999999996</AMOUNT>
