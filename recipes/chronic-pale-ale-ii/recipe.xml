      <HOP>
        <NAME>Amarillo (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>7.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.0056699061161849165</AMOUNT>
