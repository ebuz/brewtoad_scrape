    <HOPS>
      <HOP>
        <NAME>Fuggle (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>4.75</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.0170097</AMOUNT>
