      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>15.4</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.008504859174277372</AMOUNT>
