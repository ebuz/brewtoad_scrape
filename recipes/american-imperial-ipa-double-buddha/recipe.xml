      <HOP>
        <NAME>Cascade (NZ)</NAME>
        <ORIGIN>New Zealand</ORIGIN>
        <ALPHA>5.5</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.02099998287008224</AMOUNT>
