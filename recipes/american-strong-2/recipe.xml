    <HOPS>
      <HOP>
        <NAME>Zythos (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>10.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.04255259949999999</AMOUNT>
