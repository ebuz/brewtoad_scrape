      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>15.4</ALPHA>
        <BETA></BETA>
        <AMOUNT>2.0000021574225</AMOUNT>
