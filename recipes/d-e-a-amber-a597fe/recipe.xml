    <HOPS>
      <HOP>
        <NAME>Chinook (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>10.6</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.23813579999999998</AMOUNT>
