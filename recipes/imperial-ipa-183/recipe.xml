    <HOPS>
      <HOP>
        <NAME>Summit (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>18.1</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.056699</AMOUNT>
