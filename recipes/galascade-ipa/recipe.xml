    <HOPS>
      <HOP>
        <NAME>Galena (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>13.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>25.0</AMOUNT>
