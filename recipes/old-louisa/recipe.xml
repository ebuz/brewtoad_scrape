    <HOPS>
      <HOP>
        <NAME>Magnum (DE)</NAME>
        <ORIGIN>Germany</ORIGIN>
        <ALPHA>12.6</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.06378644380708023</AMOUNT>
        <DISPLAY_AMOUNT>2.25000242710031 oz</DISPLAY_AMOUNT>
