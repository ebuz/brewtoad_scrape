    <HOPS>
      <HOP>
        <NAME>Columbus (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>15.0</ALPHA>
        <BETA></BETA>
        <AMOUNT>30.0</AMOUNT>
