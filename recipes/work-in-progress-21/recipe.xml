    <HOPS>
      <HOP>
        <NAME>Citra (US)</NAME>
        <ORIGIN>United States</ORIGIN>
        <ALPHA>14.5</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.022679624464739666</AMOUNT>
