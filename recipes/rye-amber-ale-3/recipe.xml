    <HOPS>
      <HOP>
        <NAME>Brewers Gold (DE)</NAME>
        <ORIGIN>Germany</ORIGIN>
        <ALPHA>9.1</ALPHA>
        <BETA></BETA>
        <AMOUNT>0.021262124999999996</AMOUNT>
